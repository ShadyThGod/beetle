{
    "name": "Beetle",
    "version": "1.0",
    "description": "Get logins from BugMeNot easily",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "img/icon32.png",
            "48": "img/icon48.png",
            "128": "img/icon128.png"
        }
    },
    "permissions": ["activeTab", "http://bugmenot.com/", "clipboardWrite"],
    "manifest_version": 2
}